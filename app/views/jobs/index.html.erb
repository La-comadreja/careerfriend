<h1>Listing jobs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Location</th>
    <th>Level</th>
    <th>Reward</th>
    <th>Rewardtype</th>
    <th>Company</th>
    <th>Description</th>
    <th>Skills</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.name %></td>
    <td><%= job.location %></td>
    <td><%= job.level %></td>
    <td><%= job.reward %></td>
    <td><%= job.rewardtype %></td>
    <td><%= job.company %></td>
    <td><%= job.description %></td>
    <td><%= job.skills %></td>
    <td><%= link_to 'Show', job %></td>
    <td><%= link_to 'Edit', edit_job_path(job) %></td>
    <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Job', new_job_path %>
